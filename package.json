{
  "name": "fumbblui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode localdev",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build --outDir ../dist --mode dev",
    "deploy": "vite build --outDir ../distlive --mode live",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "team-format-check": "prettier src/pages/team --check",
    "team-format": "prettier src/pages/team --write",
    "format-all": "prettier --write \"src/**/*.{js,ts,vue}\""
  },
  "dependencies": {
    "@observablehq/plot": "^0.6.14",
    "@types/d3": "^7.4.3",
    "axios": "^1.7.2",
    "d3": "^7.9.0",
    "pinia": "^2.1.7",
    "sortablejs": "^1.15.2",
    "sortablejs-vue3": "^1.2.11",
    "tippy.js": "^6.3.7",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/color": "^3.0.6",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "color": "^4.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-ts": "^1.0.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.1",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "vite-plugin-handlebars": "^2.0.0",
    "vitest": "^1.6.0",
    "vue-facing-decorator": "^3.0.4",
    "vue-loading-overlay": "^6.0.4",
    "vue-tsc": "^2.0.21"
  }
}
